<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Entities.ArtImgEntity" table="ART_IMG" schema="">
        <id name="idArtImg">
            <column name="ID_ART_IMG" sql-type="bigint(19)"/>
        </id>
        <property name="idbaseArtImg">
            <column name="IDBASE_ART_IMG" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextBaseArtImg">
            <column name="IDEXT_BASE_ART_IMG" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextDoutArtImg">
            <column name="IDEXT_DOUT_ART_IMG" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="typeTblArtImg">
            <column name="TYPE_TBL_ART_IMG" sql-type="smallint(5)" not-null="true"/>
        </property>
        <property name="guidArtImg">
            <column name="GUID_ART_IMG" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="posIsmArtImg">
            <column name="POS_ISM_ART_IMG" sql-type="timestamp(19)" not-null="true"/>
        </property>
        <property name="idgrpArtImg">
            <column name="IDGRP_ART_IMG" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="guidgrpArtImg">
            <column name="GUIDGRP_ART_IMG" sql-type="char(38)" length="38" not-null="true"/>
        </property>
        <property name="nameArtImg">
            <column name="NAME_ART_IMG" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="textArtImg">
            <column name="TEXT_ART_IMG" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="heightArtImg">
            <column name="HEIGHT_ART_IMG" sql-type="integer" not-null="true"/>
        </property>
        <property name="widthArtImg">
            <column name="WIDTH_ART_IMG" sql-type="integer" not-null="true"/>
        </property>
        <property name="imageArtImg">
            <column name="IMAGE_ART_IMG" sql-type="blob sub_type binary(0)" not-null="true"/>
        </property>
        <property name="indexArtImg">
            <column name="INDEX_ART_IMG" sql-type="integer" not-null="true"/>
        </property>
<!--        <many-to-one name="artImgGrpByIdgrpArtImg" class="Model.Entities.ArtImgGrpEntity">-->
<!--            <column name="IDGRP_ART_IMG" not-null="true"/>-->
<!--        </many-to-one>-->
        <set name="artIbSByIdArtImg" inverse="true">
            <key>
                <column name="IDIMG_ART_IB_S" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="Model.Entities.ArtIbSEntity" />
        </set>
    </class>
</hibernate-mapping>