<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Entities.CustKlientEntity" table="CUST_KLIENT" schema="">
        <id name="idCustKlient">
            <column name="ID_CUST_KLIENT" sql-type="bigint(19)"/>
        </id>
        <property name="idbaseCustKlient">
            <column name="IDBASE_CUST_KLIENT" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextDoutCustKlient">
            <column name="IDEXT_DOUT_CUST_KLIENT" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextBaseCustKlient">
            <column name="IDEXT_BASE_CUST_KLIENT" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="gidCustKlient">
            <column name="GID_CUST_KLIENT" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="idaccountCustKlient">
            <column name="IDACCOUNT_CUST_KLIENT" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idklientCustKlient">
            <column name="IDKLIENT_CUST_KLIENT" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="typeTblCustKlient">
            <column name="TYPE_TBL_CUST_KLIENT" sql-type="smallint(5)" not-null="true"/>
        </property>
        <many-to-one name="custAccountByIdaccountCustKlient" class="Model.Entities.CustAccountEntity" insert="false" update="false">
            <column name="IDACCOUNT_CUST_KLIENT" not-null="true"/>
        </many-to-one>
        <many-to-one name="sklientByIdklientCustKlient" class="Model.Entities.SklientEntity" insert="false" update="false">
            <column name="IDKLIENT_CUST_KLIENT" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>