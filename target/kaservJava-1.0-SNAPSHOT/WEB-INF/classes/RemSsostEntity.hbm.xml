<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Entities.RemSsostEntity" table="REM_SSOST" schema="">
        <id name="idRemssost">
            <column name="ID_REMSSOST" sql-type="bigint(19)"/>
        </id>
        <property name="gidRemssost">
            <column name="GID_REMSSOST" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="nameRemssost">
            <column name="NAME_REMSSOST" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="idbaseRemSsost">
            <column name="IDBASE_REM_SSOST" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextDoutRemSsost">
            <column name="IDEXT_DOUT_REM_SSOST" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="idextBaseRemSsost">
            <column name="IDEXT_BASE_REM_SSOST" sql-type="bigint(19)" not-null="true"/>
        </property>
        <property name="typeTblRemSsost">
            <column name="TYPE_TBL_REM_SSOST" sql-type="smallint(5)" not-null="true"/>
        </property>
        <set name="remZSByIdRemssost" inverse="true">
            <key>
                <column name="IDSOST_REM_Z" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="Model.Entities.RemZEntity"/>
        </set>
<!--        <set name="remZDvsByIdRemssost" inverse="true">-->
<!--            <key>-->
<!--                <column name="IDSOST_REM_Z_DV" not-null="true"/>-->
<!--            </key>-->
<!--            <one-to-many not-found="ignore" class="Model.Entities.RemZDvEntity"/>-->
<!--        </set>-->
    </class>
</hibernate-mapping>